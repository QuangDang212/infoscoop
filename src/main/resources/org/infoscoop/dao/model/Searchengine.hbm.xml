<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Searchengine"
		table="IS_SEARCHENGINES"
	>
		
        <composite-id name="Id" class="SearchenginePK">
            <key-property
                name="Temp"
                column="TEMP"
                type="integer"
            />
            <key-property
                name="Squareid"
                column="square_id"
                type="string"
                length="64"
            />
        </composite-id>
		
		<property
			name="Data"
			column="DATA"
			type="text"
			not-null="true"
		/>
	</class>
	<sql-query name="is_searchengines.copySquare">
		<![CDATA[
INSERT
	INTO
		IS_SEARCHENGINES(
			temp,
			data,
			square_id
		)
	SELECT
		s.temp,
		s.data,
		:squareId
	FROM
		IS_SEARCHENGINES as s
	WHERE
		s.square_id = :defaultSquareId
		]]>
	</sql-query>
</hibernate-mapping>
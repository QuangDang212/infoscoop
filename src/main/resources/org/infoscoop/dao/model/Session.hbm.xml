<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Session"
		table="IS_SESSIONS"
	>
		<meta attribute="sync-DAO">false</meta>
        <composite-id name="Id" class="SessionPK">
            <key-property
                name="Uid"
                column="`UID`"
                type="string"
            />
            <key-property
                name="Squareid"
                column="square_id"
                type="string"
                length="64"
            />
        </composite-id>

		<property
			name="Sessionid"
			column="SESSIONID"
			type="string"
			not-null="true"
			length="256"
		/>

		<property
			name="Logindatetime"
			column="LOGINDATETIME"
			type="timestamp"
		/>

	</class>	
</hibernate-mapping>
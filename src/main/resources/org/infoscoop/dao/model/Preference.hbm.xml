<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class name="Preference" table="IS_PREFERENCES">
        <composite-id name="Id" class="PreferencePK">
            <key-property
                name="Uid"
                column="`UID`"
                type="string"
            />
            <key-property
                name="Squareid"
                column="square_id"
                type="string"
                length="64"
            />
        </composite-id>

		<property
			name="Data"
			column="DATA"
			type="text"
			not-null="true"
		/>

	</class>
</hibernate-mapping>
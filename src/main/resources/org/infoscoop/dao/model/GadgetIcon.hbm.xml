<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="GadgetIcon"
		table="IS_GADGET_ICONS"
	>
		<meta attribute="sync-DAO">false</meta>
        <composite-id name="Id" class="GadgetIconPK">
            <key-property
                name="Type"
                column="TYPE"
                type="string"
            />
            <key-property
                name="Squareid"
                column="square_id"
                type="string"
                length="64"
            />
        </composite-id>

		<property
			name="Url"
			column="URL"
			type="string"
			length="1024"
		/>


	</class>
</hibernate-mapping>
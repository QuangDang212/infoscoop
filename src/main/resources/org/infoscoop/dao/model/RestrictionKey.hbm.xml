<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="RestrictionKey"
		table="IS_RESTRICTION_KEYS"
	>
        <meta attribute="sync-DAO">false</meta>
        <id
            name="Id"
            type="string"
            column="ID"
        >
        </id>
		<property
			name="Expired"
			column="expired"
			type="timestamp"
			not-null="true"
		/>
		<property
			name="Uid"
			column="`UID`"
			type="string"
			not-null="true"
			length="150"
		/>
	</class>	
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="ExternalAPIKey"
		table="IS_EXTERNALAPI_KEYS"
	>
        <meta attribute="sync-DAO">false</meta>
        <id
            name="Id"
            type="string"
            column="ID"
        >
        </id>
		<property
			name="ApiKey"
			column="api_key"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="ApiSecret"
			column="api_secret"
			type="string"
			not-null="false"
			length="255"
		/>
	</class>	
</hibernate-mapping>